{
    "manifest_version": 2,
    "name": "unmedium",
    "version": "0.1",
    "description": "Makes medium usable.",
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "unmedium.js"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html"
    },
    "permissions": [
        "storage"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "{079a2d89-6a27-471c-a2c4-acb0ed6f21ee}"
        }
    }
}